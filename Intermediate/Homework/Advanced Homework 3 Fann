2.1
class Vehicle(ABC):
    @abstractmethod
    def start_engine(self,start):
        self.start = start
        print("Car started engine")

    @abstractmethod
    def stop_engine(self, stop):
        self.stop = stop
        stop = random.randint(1,6)
        print(f"Car stopped {stop} seconds")

    @abstractmethod
    def drive(self,drive):
        self.drive = drive
        drive = random.randint(1,10)
        print(f"Car drove {drive} feet")

2.2
class Car(Vehicle):
    Car.start_engine()
    Car.stop_engine()
    Car.drive()

2.3
class Car(Vehicle):
    Car.start_engine()
    Car.stop_engine()
    Car.drive()
